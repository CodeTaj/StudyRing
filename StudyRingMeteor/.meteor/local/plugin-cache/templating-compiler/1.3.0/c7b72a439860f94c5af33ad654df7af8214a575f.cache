{"head":"","body":"","js":"\nTemplate.__checkName(\"AccountSettings\");\nTemplate[\"AccountSettings\"] = new Template(\"Template.AccountSettings\", (function() {\n  var view = this;\n  return [ HTML.Raw(\"<h1> Account settings </h1>\\n  \"), Blaze.If(function() {\n    return Spacebars.dataMustache(Spacebars.dot(view.lookup(\"$\"), \"Session\", \"get\"), \"changeSettings\");\n  }, function() {\n    return [ \"\\n    \", HTML.I({\n      class: \"fa fa-close\"\n    }), \"\\n    \", HTML.FORM(\"\\n      Old password: \", HTML.INPUT({\n      type: \"password\",\n      name: \"oldPassword\",\n      id: \"oldPassword\"\n    }), \" \", HTML.BR(), \"\\n      Change password: \", HTML.INPUT({\n      type: \"password\",\n      name: \"newPassword\",\n      id: \"newPassword\"\n    }), \" \", HTML.BR(), \"\\n      Rewrite password: \", HTML.INPUT({\n      type: \"password\",\n      name: \"newPassword2\",\n      id: \"newPassword2\"\n    }), \" \", HTML.BR(), \"\\n      \", HTML.INPUT({\n      type: \"submit\",\n      class: \"changePassword\",\n      name: \"changePassword\",\n      id: \"changePassword\"\n    }), \"\\n    \"), \"\\n    \", HTML.FORM(\"\\n      Change username: \", HTML.INPUT({\n      type: \"text\",\n      name: \"newUsername\"\n    }), \" \", HTML.BR(), \"\\n      \", HTML.INPUT({\n      type: \"submit\",\n      class: \"changeUsername\",\n      name: \"changeUsername\",\n      id: \"changeUsername\"\n    }), \"\\n    \"), \"\\n  \" ];\n  }, function() {\n    return [ \"\\n      Email: \", Blaze.Each(function() {\n      return Spacebars.call(Spacebars.dot(view.lookup(\"user\"), \"emails\"));\n    }, function() {\n      return [ \"\\n        \", Blaze.View(\"lookup:address\", function() {\n        return Spacebars.mustache(view.lookup(\"address\"));\n      }), \"\\n      \" ];\n    }), \" \", HTML.BR(), \"\\n      Username: \", Blaze.View(\"lookup:user.username\", function() {\n      return Spacebars.mustache(Spacebars.dot(view.lookup(\"user\"), \"username\"));\n    }), \" \", HTML.BR(), \"\\n      \", HTML.BUTTON({\n      class: \"change-settings\"\n    }, \"Change Settings\"), \"\\n  \" ];\n  }) ];\n}));\n","bodyAttrs":{}}